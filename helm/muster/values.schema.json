                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array"
        },
        "volumeMounts": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "mountPath": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            }
        },
        "volumes": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "emptyDir": {
                        "type": "object"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
